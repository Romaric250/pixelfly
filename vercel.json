{
  "version": 2,
  "builds": [
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "backend",
    "FLASK_ENV": "production"
  },
  "functions": {
    "backend/api/index.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["iad1"],
  "github": {
    "autoAlias": false
  }
}
