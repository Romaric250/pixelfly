{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "backend",
    "FLASK_ENV": "production"
  },
  "functions": {
    "backend/api/index.py": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "regions": ["iad1"]
}
